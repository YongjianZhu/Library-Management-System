<p id="notice"><%= notice %></p>

<h1>Borrowhistories</h1>

<table>
  <thead>
    <tr>
      <th>Library</th>
      <th>University</th>
      <th>Student</th>
      <th>Book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @borrowhistories.each do |borrowhistory| %>
      <tr>
        <td><%= borrowhistory.library_id %></td>
        <td><%= borrowhistory.university_id %></td>
        <td><%= borrowhistory.student_id %></td>
        <td><%= borrowhistory.book_id %></td>
        <td><%= link_to 'Show', borrowhistory %></td>
        <td><%= link_to 'Edit', edit_borrowhistory_path(borrowhistory) %></td>
        <td><%= link_to 'Destroy', borrowhistory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Borrowhistory', new_borrowhistory_path %>
