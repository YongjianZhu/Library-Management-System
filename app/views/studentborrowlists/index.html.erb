<p id="notice"><%= notice %></p>

<h1>Studentborrowlists</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Book</th>
      <th>Duedate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @studentborrowlists.each do |studentborrowlist| %>
      <tr>
        <td><%= studentborrowlist.student_id %></td>
        <td><%= studentborrowlist.book_id %></td>
        <td><%= studentborrowlist.duedate %></td>
        <td><%= link_to 'Show', studentborrowlist %></td>
        <td><%= link_to 'Edit', edit_studentborrowlist_path(studentborrowlist) %></td>
        <td><%= link_to 'Destroy', studentborrowlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Studentborrowlist', new_studentborrowlist_path %>
